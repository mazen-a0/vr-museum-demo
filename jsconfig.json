{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "ESNext",
        "target": "ESNext",
        "checkJs": true,   
        "isolatedModules": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
       
    },
    "exclude": [
        "./dist"
    ]
  }